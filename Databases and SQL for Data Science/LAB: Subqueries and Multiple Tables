--QUERY 1--
SELECT EMP_ID,F_NAME,L_NAME 
FROM EMPLOYEES
WHERE EMP_ID IN 
(SELECT EMPL_ID FROM JOB_HISTORY
WHERE YEAR(START_DATE)>2010);

--QUERY 2--
SELECT F_NAME,L_NAME,SALARY 
FROM EMPLOYEES
WHERE SALARY>
(SELECT AVG(SALARY) FROM EMPLOYEES);

--QUERY 3--
SELECT E.F_NAME,E.L_NAME,E.SALARY, E.DEP_ID, D.DEPT_ID_DEP, D.DEP_NAME
FROM EMPLOYEES E, DEPARTMENTS D
WHERE E.DEP_ID=D.DEPT_ID_DEP
ORDER BY E.SALARY DESC, D.DEP_NAME DESC;
